{
  "name": "flex-iframe",
  "version": "1.0.0",
  "description": "A flexible iframe library for seamless integration of third-party content",
  "homepage": "https://github.com/payhon/flex-iframe",
  "repository": {
    "type": "git",
    "url": "https://github.com/payhon/flex-iframe.git"
  },
  "main": "dist/flex-iframe.umd.js",
  "module": "dist/flex-iframe.es.js",
  "unpkg": "dist/flex-iframe.umd.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:js": "vite build",
    "build": "npm run build:types && npm run build:js",
    "preview": "vite preview",
    "test": "jest"
  },
  "keywords": [
    "iframe",
    "flexible",
    "integration"
  ],
  "author": "payhon",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.24",
    "@types/react": "^17.0.0",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "jest": "^29.7.0",
    "jsdom": "^25.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue": "^3.0.0",
    "vue2": "npm:vue@^2.6.14",
    "@vue/compiler-sfc": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^3.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}